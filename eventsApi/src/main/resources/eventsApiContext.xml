<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="dataAccessContext.xml"/>

    <bean id="eventService" class="org.ivt.agregator.service.EventServiceMockImpl"/>

    <bean class="org.ivt.agregator.rest.GetController">
        <constructor-arg ref="eventService"/>
    </bean>
    <bean class="org.ivt.agregator.rest.VkCodeController">
        <constructor-arg ref="parameterDao"/>
    </bean>
</beans>